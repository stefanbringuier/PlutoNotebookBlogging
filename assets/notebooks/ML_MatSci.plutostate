    if (graphDiv.data==undefined)
    {
        Plotly.newPlot(uuid, data,layout, {modeBarButtons: buttons,})
    }
    else
    {
        Plotly.react(uuid, graphDiv.data,layout, {modeBarButtons: buttons,})
    }

    
}

